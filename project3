# using for loop for print in range 10
for num in range(10):
print(num)

#Print sum of all even numbers from 10 to 20
	sum = 0
for i in range(10,20,2):
    sum = sum + i
print(sum)

#Calculate the square of each number of list (numbers-1,2,3,4,5)
numbers =[1,2,3,4,5]
for i in numbers:
    square = i ** 2
    print("square of:", i, "is:", square)
	
#Calculate the average of list of numbers
a = [2,4,6,8,10]
total_sum = 0
for i in a:
    total_sum += i or( total_sum = total_sum + i)
    avg = total_sum/len(a)
print(avg)

or 
a = [10,20,30,40,50]
sum = 0
for i in a:
    sum = sum + i
list_size = len(a)
average = sum/list_size
print(average)

#Nested Loops
line_to_print = '' #the empty string
for i in range(1,10): #defines the number to print
    for j in range(0,2): #defines the number of times to append the number to the string
        line_to_print += str(i)
    print(line_to_print)
    line_to_print = '' #reset back to an empty string
	




